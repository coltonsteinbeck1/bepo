{
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.687.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.16.1",
    "@supabase/supabase-js": "^2.44.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.2",
    "discord.js": "^14.21.0",
    "dotenv": "^16.3.1",
    "gifwrap": "^0.10.1",
    "libsodium-wrappers": "^0.7.13",
    "openai": "^4.19.0",
    "ora": "^7.0.1",
    "play-dl": "^1.9.7",
    "sharp": "^0.34.2",
    "yargs": "^17.7.2",
    "youtube-dl-exec": "^3.0.22",
    "yt-dlp-wrap": "^2.3.12",
    "ytdl-core": "^4.11.5"
  },
  "name": "bepo",
  "version": "2.0.0",
  "description": "A Discord bot with game notification services, voice interaction, and comprehensive monitoring capabilities",
  "type": "module",
  "main": "./src/bot.js",
  "author": "CodeBreaker258",
  "license": "MIT",
  "scripts": {
    "deploy": "node scripts/deploy-commands.js",
    "delete": "node scripts/delete-commands.js",
    "generate": "node src/dalle/cli.js generate",
    "start": "node src/bot.js",
    "start:quick": "./start-bepo.sh",
    "start:full": "node src/bot.js & node scripts/monitor-service.js & node scripts/offline-response-system.js",
    "start:bot-only": "./start-bot-only.sh",
    "start:bot-direct": "node src/bot.js",
    "start:monitor": "node scripts/monitor-service.js",
    "start:offline": "node scripts/offline-response-system.js",
    "stop": "./stop-bepo.sh",
    "stop:all": "pkill -f 'node.*src/bot.js' || true; pkill -f 'node.*monitor-service.js' || true; pkill -f 'node.*offline-response-system.js' || true",
    "stop:bot": "./stop-bot-only.sh",
    "stop:monitor": "pkill -f 'node.*monitor-service.js' || true",
    "stop:offline": "pkill -f 'node.*offline-response-system.js' || true",
    "restart": "npm run stop && sleep 2 && npm run start",
    "dev": "node src/bot.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "prestart": "npm run test",
    "prebuild": "npm run test",
    "monitor": "node scripts/bot-monitor.js",
    "setup-webhooks": "node scripts/setup-webhook-notifications.js",
    "test-notifications": "node scripts/test-offline-notifications.js",
    "test-scenarios": "node scripts/test-offline-notifications.js scenarios",
    "status": "./scripts/bepo-status.sh",
    "status:detailed": "node scripts/check-bot-status-detailed.js",
    "setup-offline-responses": "node scripts/setup-offline-responses.js",
    "offline-monitor": "node scripts/offline-response-system.js",
    "logs": "tail -f logs/monitor.log",
    "logs:health": "tail -f logs/health-$(date +%Y-%m-%d).json",
    "logs:bot": "tail -f logs/serverOutput.log",
    "logs:monitor": "tail -f logs/monitorOutput.log",
    "logs:offline": "tail -f logs/offlineOutput.log",
    "logs:all": "tail -f logs/serverOutput.log logs/monitorOutput.log logs/offlineOutput.log",
    "validate-offline": "node scripts/validate-offline-setup.js"
  },
  "bin": {
    "bepo": "./src/cli.js"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "supabase": "^1.123.4",
    "vitest": "^3.2.4"
  }
}
